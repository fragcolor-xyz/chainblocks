@mesh(root)

@wire(upload-proto {
  ExpectBytes
  "Hello" = proto-data
  "Namaste"
} Pure: true)

@wire(main0 {
  "Proto-Indo-European" = proto-data
  proto-data | ToBytes | Do(upload-proto) | Log
  proto-data | Log |
  Assert.Is("Proto-Indo-European" true)
})

@wire(main-pure {
  2 >= value
  ForRange(0 10 {
    value | Math.Add(value) > value
  })
  value | Log("Result") | Assert.Is(4096 true)
} Pure: true)

@wire(pure0 {
  2 >= value
  ForRange(0 10 {
    value | Math.Add(value) > value
  })
  value | Log("Result") | Assert.Is(4096 true)
} Pure: true)

@wire(main1 {
  4 >= value
  Do(pure0)
})

@schedule(root main0)
@schedule(root main1)
@run(root FPS: 10) | Assert.Is(true)