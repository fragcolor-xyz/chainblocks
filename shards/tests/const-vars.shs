@mesh(main)

@wire(test {
    10 = t10
    20 = t20
    [t10 t20] | Math.Add([40 30]) |
    Assert.Is([50 50] true)
    ; {"Hello" t10
    ;  "World" t20} | Take(0) | Assert.Is(10 true)
    [t10 t20] | Take(0) | Assert.Is(10 true)
} Looped: true)

@schedule(main test)
@run(main 0.2 10)