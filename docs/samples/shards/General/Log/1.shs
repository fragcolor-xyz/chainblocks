"I am a var" = var
Math.Multiply(2 4) | Log ; log previous shard output
var | Log  ; log a variable value
Math.Add(3 9) | Log(Prefix: "String") ; prefix a string to the logged output

"I am a var" = var
Math.Multiply(2 4) | Log(Level: LogLevel.Info) ; log previous shard output
var | Log(Level: LogLevel.Warning)  ; log a variable value
Math.Add(3 9) | Log(Prefix: "String" Level: LogLevel.Error) ; prefix a string to the logged output
