@wire(ui-wire {
  UI.Window(
    Title: "UI Frame"
    Position: @f2(0 0)
    Anchor: Anchor::Center
    Width: 700
    Height: 400
    Contents: {
      {
        UI.Frame(
          InnerMargin: @f4(10) OuterMargin: @f4(0) Rounding: @f4(5)
          FillColor: @color(50 50 21)
          StrokeColor: @color(250 250 32) StrokeWidth: 2.0
          Contents:
          {
            "Frame content" | UI.Label
          }
        )
      }
    }
  )
} Looped: true)

{ui-behavior: ui-wire}