GFX.MainWindow(Contents: {
  ; Setup
  Table(ui-draw-queue Type: GFX.DrawQueue)
  GFX.UIPass(ui-draw-queue) >> render-steps

  ; UI
  UI({
    UI.CentralPanel({
      0.0 >= progress
      progress | UI.ProgressBar(Overlay: "Progress")
      Math.Add(0.01) > progress
    })
  })

  GFX.Render(Steps: render-steps)
})
