@wire(ui-wire {
  UI.Window(
    Title: "UI PlotBar"
    Position: @f2(0 0)
    Anchor: Anchor::Center
    Width: 700
    Height: 400
    Contents: {
      UI.CentralPanel(
        UI.Plot({
          [@f2(-1) @f2(0) @f2(1)] | UI.PlotLine
        })
      )
    }
  )
} Looped: true)

{ui-behavior: ui-wire}