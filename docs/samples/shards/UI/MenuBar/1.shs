@wire(ui-wire {
  UI.Window(
    Title: "UI MenuBar"
    Position: @f2(0 0)
    Anchor: Anchor::Center
    Width: 700
    Height: 400
    Contents: {
      UI.TopPanel(
        Contents: {
          UI.TopPanel(
            Contents:
            UI.MenuBar(
              Contents:
              UI.Menu(
                "Menu"
                {"Some Text" | UI.Label}
              )
            )
          )
        }
      )
    }
  )
} Looped: true)

{ui-behavior: ui-wire}