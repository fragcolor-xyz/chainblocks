; Simple collapsing
GFX.MainWindow(Contents: {
  ; Setup
  GFX.DrawQueue = ui-draw-queue
  GFX.UIPass(ui-draw-queue) >> render-steps

  ; UI
  UI({
    UI.CentralPanel(Contents: {
      UI.Collapsing(
        Heading: "Heading"
        DefaultOpen: true
        Contents: {
          "Content" | UI.Label
        }
      )
    })
  })

  UI.Render(ui-draw-queue)
  GFX.Render(Steps: render-steps)
})
