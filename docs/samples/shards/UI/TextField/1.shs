
@wire(ui-wire {
  UI.Window(
    Position: @f2(0 0)
    Anchor: Anchor::Center
    Width: 200
    Height: 200
    Flags: [WindowFlags::NoResize]
    Contents: {
      Once({
        "Lorem ipsum dolor sit amet" >= text
      })
      UI.TextField(
        Variable: text
        Multiline: false
      )
    }
  )
} Looped: true)

{ui-behavior: ui-wire}