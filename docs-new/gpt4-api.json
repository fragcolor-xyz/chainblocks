{
  "openapi": "3.1.0",
  "info": {
    "title": "Get Shards API info",
    "description": "Retrieves information about shards, the building blocks of the Shards language.",
    "version": "v1.0.0"
  },
  "servers": [
    {
      "url": "https://shards.formabble.com"
    }
  ],
  "paths": {
    "/shards-fuzzy-search": {
      "get": {
        "description": "Get related Shards by fuzzy search, it will search in names, parameters, variable names, descriptions and more",
        "operationId": "FuzzyShards",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "The words to search for",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "deprecated": false
      }
    }
  },
  "components": {
    "schemas": {}
  }
}